<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" prefHeight="495.0" prefWidth="690.0" stylesheets="@/seedu/addressbook/ui/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.addressbook.ui.MainWindow">

    <children>

        <TextArea fx:id="outputConsole" editable="false" prefHeight="495.0" prefWidth="690.0" wrapText="true" VBox.vgrow="ALWAYS">
        </TextArea>
      <HBox prefHeight="25.0" prefWidth="200.0" style="-fx-background-color: gray;">
         <children>
      
              <TextField fx:id="commandInput" minWidth="31.0" onAction="#onCommand" prefHeight="25.0" prefWidth="720.0" promptText="Enter command here">
        </TextField>
          <Button fx:id="clearOutput" minWidth="0.0" mnemonicParsing="false" onAction="#clearOutputConsole" prefHeight="27.0" prefWidth="90.0" text=" Clear Screen" textAlignment="CENTER" />
         </children>
      </HBox>

    </children>

</VBox>
