<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="center" stylesheets="@/seedu/addressbook/ui/DarkTheme.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.addressbook.ui.MainWindow">

    <children>

        <TextField fx:id="commandInput" onAction="#onCommand" VBox.vgrow="NEVER">
         <font>
            <Font name="System Italic" size="20.0" />
         </font></TextField>
      <Button mnemonicParsing="false" text="search" onAction="#onCommand">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Button>

        <TextArea fx:id="outputConsole" editable="false" wrapText="true" VBox.vgrow="ALWAYS">
         <font>
            <Font name="Cambria" size="20.0" />
         </font></TextArea>

    </children>

</VBox>
