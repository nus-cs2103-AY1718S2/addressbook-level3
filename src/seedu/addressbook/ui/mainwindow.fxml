<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<VBox stylesheets="@/seedu/addressbook/ui/DarkTheme.css" alignment="center" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="seedu.addressbook.ui.MainWindow">

    <HBox VBox.vgrow="NEVER">
        <TextField fx:id="commandInput" onAction="#onCommand" HBox.hgrow="always">
        </TextField>

        <Button onAction="#onCommand"/>
    </HBox>

    <TextArea fx:id="outputConsole" editable="false" wrapText="true" VBox.vgrow="ALWAYS">
    </TextArea>
</VBox>
