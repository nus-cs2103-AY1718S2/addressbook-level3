<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="center" stylesheets="@/seedu/addressbook/ui/DarkTheme.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.addressbook.ui.MainWindow">

    <children>

        <TextField fx:id="commandInput" onAction="#onCommand" promptText="enter command here" style="-fx-background-color: black;" VBox.vgrow="NEVER">
         <font>
            <Font name="Agency FB" size="12.0" />
         </font></TextField>

        <TextArea fx:id="outputConsole" editable="false" stylesheets="@DarkTheme.css" wrapText="true" VBox.vgrow="ALWAYS">
         <font>
            <Font name="STLiti" size="12.0" />
         </font></TextArea>

    </children>

</VBox>
